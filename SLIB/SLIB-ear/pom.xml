<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>ec.com</groupId>
		<artifactId>SLIB</artifactId>
		<version>1.1-SNAPSHOT</version>
	</parent>
	<artifactId>SLIB-ear</artifactId>
	<name>Internet Ear</name>
	<packaging>ear</packaging>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-ear-plugin</artifactId>
				<version>2.10.1</version>
				<configuration>
					<defaultLibBundleDir>lib</defaultLibBundleDir>
					<modules>
						<webModule>
							<groupId>ec.com</groupId>
							<artifactId>SLIB-internet-web</artifactId>
							<bundleFileName>SLIB-internet-web.war</bundleFileName>
							<contextRoot>/SLIB-internet-web</contextRoot>
						</webModule>
						<webModule>
							<groupId>ec.com</groupId>
							<artifactId>SLIB-intranet-web</artifactId>
							<bundleFileName>SLIB-intranet-web.war</bundleFileName>
							<contextRoot>/SLIB</contextRoot>
						</webModule>
						<webModule>
							<groupId>ec.com</groupId>
							<artifactId>SLIB-internet-ws</artifactId>
							<bundleFileName>SLIB-internet-ws.war</bundleFileName>
							<contextRoot>/SLIB-internet-ws</contextRoot>
						</webModule>
					</modules>
					<archive>
						<manifestEntries>
							<Dependencies></Dependencies>
						</manifestEntries>
					</archive>
					
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-internet-web</artifactId>
			<version>1.1-SNAPSHOT</version>
			<type>war</type>
		</dependency>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-intranet-web</artifactId>
			<version>1.1-SNAPSHOT</version>
			<type>war</type>
		</dependency>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-internet-ws</artifactId>
			<version>1.1-SNAPSHOT</version>
			<type>war</type>
		</dependency>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-comun</artifactId>
			<version>1.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.hibernate</groupId>
			<artifactId>hibernate-entitymanager</artifactId>
			<version>4.2.3.Final</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.hibernate.javax.persistence</groupId>
			<artifactId>hibernate-jpa-2.0-api</artifactId>
			<version>1.0.1.Final</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.hibernate</groupId>
			<artifactId>hibernate-core</artifactId>
			<version>4.2.4.Final</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-api</artifactId>
			<version>1.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-negocio</artifactId>
			<version>1.1-SNAPSHOT</version>
			<type>ejb</type>
		</dependency>
		<dependency>
			<groupId>ec.com</groupId>
			<artifactId>SLIB-persistencia</artifactId>
			<version>1.1-SNAPSHOT</version>
			<type>ejb</type>
		</dependency>
	</dependencies>
</project>